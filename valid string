#include <stdio.h>
#include <ctype.h>

int main() {
    char s[200];
    int i = 0, ok = 1;

    scanf("%199s", s);

    while (s[i] != 0) {
        if (!isalnum(s[i])) {
            ok = 0;
            break;
        }
        i++;
    }

    if (ok)
        printf("valid");
    else
        printf("invalid");

    return 0;
}
