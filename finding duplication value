#include <stdio.h>

int main() {
    int n, i, j, duplicate;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter elements of the array:\n");
    for (i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("Duplicate elements: ");
    int foundDuplicate = 0;

    for (i = 0; i < n; i++) {
        duplicate = 0;
        for (j = 0; j < i; j++) {
            if (arr[i] == arr[j]) {
                duplicate = 1;
                break;
            }
        }
        if (!duplicate) {
            int count = 0;
            for (j = i + 1; j < n; j++) {
                if (arr[i] == arr[j])
                    count++;
            }
            if (count > 0) {
                printf("%d ", arr[i]);
                foundDuplicate = 1;
            }
        }
    }

    if (!foundDuplicate)
        printf("None");

    printf("\n");
    return 0;
}
