#include <stdio.h>

int main() {
    int n, i, key, low, high, mid, found = 0;

    printf("Enter number of register numbers: ");
    scanf("%d", &n);

    int reg[n];

    printf("Enter the register numbers in sorted order:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &reg[i]);
    }

    printf("Enter register number to search: ");
    scanf("%d", &key);

    low = 0;
    high = n - 1;

    while (low <= high) {
        mid = (low + high) / 2;

        if (reg[mid] == key) {
            found = 1;
            break;
        } else if (reg[mid] < key) {
            low = mid + 1;
        } else {
            high = mid - 1;
        }
    }

    if (found)
        printf("Register number found at position %d\n", mid + 1);
    else
        printf("Register number not found\n");

    return 0;
}
