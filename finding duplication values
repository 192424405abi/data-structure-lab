#include <stdio.h>

int main() {
    int n, i, j, found;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n];

    printf("Enter the elements:\n");
    for (i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("Duplicate elements: ");
    int duplicates = 0;

    for (i = 0; i < n; i++) {
        found = 0;
        for (j = 0; j < i; j++) {
            if (arr[i] == arr[j]) {
                found = 1;
                break;
            }
        }
        if (!found) {
            int count = 0;
            for (j = i + 1; j < n; j++) {
                if (arr[i] == arr[j])
                    count++;
            }
            if (count > 0) {
                printf("%d ", arr[i]);
                duplicates = 1;
            }
        }
    }

    if (!duplicates)
        printf("None");

    printf("\n");
    return 0;
}
