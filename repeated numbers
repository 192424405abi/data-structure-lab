#include <stdio.h>

int main() {
    int n, i, j, found;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    int a[n];

    printf("Enter the elements:\n");
    for (i = 0; i < n; i++)
        scanf("%d", &a[i]);

    printf("Repeated numbers: ");
    int repeated = 0;

    for (i = 0; i < n; i++) {
        found = 0;
        for (j = 0; j < i; j++) {
            if (a[i] == a[j]) {
                found = 1;
                break;
            }
        }
        if (!found) {
            int count = 0;
            for (j = i + 1; j < n; j++) {
                if (a[i] == a[j])
                    count++;
